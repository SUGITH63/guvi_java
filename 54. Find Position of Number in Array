import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
       Scanner  sc=new Scanner(System.in);
       int a=sc.nextInt();
       int b=sc.nextInt();
       int arr[]=new int[a];
       for(int i=0;i<a;i++)
       {
           arr[i]=sc.nextInt();
       }
       boolean f=false;
       for(int i=0;i<a;i++)
       {
           if(arr[i]==b)
           {
               f=true;
               System.out.println(i+1);
               return;
           }
       }
       if(!f){
           System.out.println("-1");
       }
    }
}