import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int s1=0;
        while(n!=0){
            int d=n%10;
            s1=s1+(d*d);
            n=n/10;
        }
        System.out.println(s1);
    }
}