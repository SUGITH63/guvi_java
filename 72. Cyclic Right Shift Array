import java.util.Scanner;
public class Main {
    static void reverseArray(int[] arr,int left,int right) {
        while (left<right) {
            int temp=arr[left];
            arr[left]=arr[right];
            arr[right]=temp;
            left++;
            right--;
        }
    }
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int arr[]=new int[a];
        int left=0;
        int right=arr.length-1;
        for(int i=0;i<a;i++)
        {
            arr[i]=sc.nextInt();
        }
        b=b%a;
        reverseArray(arr,0,a-1);
        reverseArray(arr,0,b-1);
        reverseArray(arr,b,a-1);
        boolean f=true;
        for (int i=0;i<a;i++) {
            if(!f){
                System.out.print(" ");
            }
                 System.out.print(arr[i]);
                 f=false;
        }
    }
}