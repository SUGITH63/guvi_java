import java.util.*;
public class Main{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String s = sc.nextLine().trim();
        boolean isNumeric = true;
        for (int i=0;i<s.length();i++) {
            char c = s.charAt(i);
            if (c<'0'|| c>'9') {
                isNumeric=false;
                break;
            }
        }
        System.out.println(isNumeric?"yes":"no");
    }
}
