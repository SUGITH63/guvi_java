import java.util.Scanner;
public class Main {
    public static boolean persqu(int n){
        int s=(int)Math.sqrt(n);
        if(s*s==n){
            return true;
        }else{
            return false;
        }
    }
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
       int a=sc.nextInt();
       int b=sc.nextInt();
       int c=0;
       for(int i=a;i<b;i++)
       {
           if(persqu(i))
            c++;
       }
       if(c>0)
       {
           System.out.println(c);
       }else{
            System.out.println("-1");
       }
    }
}