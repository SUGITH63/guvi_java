import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        int arr[] =new int[n];

        int evenCount=0, oddCount=0;
        int evenNum=-1, oddNum =-1;

        for (int i = 0; i < n; i++) {
            arr[i]=sc.nextInt();
            if (arr[i]%2 == 0){
                evenCount++;
                evenNum = arr[i];
            }else{
                oddCount++;
                oddNum = arr[i];
            }
        }

        if (evenCount == 1){
            System.out.println(evenNum);
        }else if (oddCount == 1){
            System.out.println(oddNum);
        }else{
            System.out.println("-1");
        }
    }
}
